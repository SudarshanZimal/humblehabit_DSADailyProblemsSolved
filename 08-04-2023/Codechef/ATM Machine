/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		while(n-->0){
		    int m=sc.nextInt();
		    int k=sc.nextInt();
		    while(m-->0){
		        int l=sc.nextInt();
		      	String str="";
		        if(k>=l){
		            k=k-l;
		            str=str+"1";
		        }
		        else{
		            str+="0";
		     
		     }
		       System.out.print(str);
		    }
		    System.out.println();
		}
	}
}
