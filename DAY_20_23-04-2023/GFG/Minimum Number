
class Solution {
    public static int minimumNumber(int n, int[] arr) {
        // code here
     int oddNumber = 0;
       for( int i = 0 , j = 0 ; i < n ; i++){
           if( arr[i] % 2 != 0  )oddNumber=1;
           
       }
       if( oddNumber == 1)return 1;
      return Math.min(arr[0] , arr[n-1]);
       
    }
}
        
